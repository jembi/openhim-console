{
	"name": "openhim-console",
	"description": "This application provides a web application to configure and manage the OpenHIM-core component.",
	"version": "1.13.2",
	"dependencies": {
		"@types/http-server": "^0.10.0",
		"http-server": "^0.11.1",
		"replace": "^1.0.1"
	},
	"devDependencies": {
		"@kariudo/angular-fullscreen": "^1.0.1",
		"@types/rimraf": "^2.0.2",
		"angular": "^1.7.9",
		"angular-bootstrap-colorpicker": "^3.0.32",
		"angular-cookies": "^1.7.5",
		"angular-file-upload": "^2.5.0",
		"angular-highlightjs": "^0.7.1",
		"angular-mocks": "^1.7.5",
		"angular-resource": "^1.7.5",
		"angular-route": "^1.7.5",
		"angular-sanitize": "^1.7.5",
		"angular-ui-bootstrap": "^2.5.0",
		"babel-core": "^6.26.3",
		"babel-loader": "^7.1.2",
		"babel-minify-webpack-plugin": "^0.3.1",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.7.0",
		"bootstrap": "^3.4.1",
		"chai": "^3.5.0",
		"clean-webpack-plugin": "^0.1.17",
		"copy-webpack-plugin": "^4.6.0",
		"crypto-js": "^3.1.9-1",
		"css-loader": "^1.0.1",
		"d3": "^5.7.0",
		"dirty-chai": "^2.0.1",
		"eonasdan-bootstrap-datetimepicker": "^4.17.47",
		"extract-text-webpack-plugin": "^3.0.2",
		"file-loader": "^1.1.5",
		"file-saver": "^1.3.3",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^2.30.1",
		"jquery": "^3.4.1",
		"json-loader": "^0.5.7",
		"karma": "^4.2.0",
		"karma-dirty-chai": "^2.0.0",
		"karma-mocha": "^1.3.0",
		"karma-mocha-reporter": "^2.2.5",
		"karma-phantomjs-launcher": "^1.0.1",
		"karma-sinon-chai": "1.2.3",
		"lodash": "^4.17.15",
		"mocha": "^5.2.0",
		"moment": "^2.23.0",
		"moment-timezone": "^0.5.23",
		"morris.js": "git+https://github.com/morrisjs/morris.js#77c24f6b70df2b23c7bd5d4d67240850d1b83005",
		"ng-file-upload": "^12.2.13",
		"phantomjs-prebuilt": "^2.1.16",
		"pretty-data": "^0.40.0",
		"raphael": "^2.2.7",
		"rimraf": "^2.6.2",
		"sinon": "^1.17.4",
		"sinon-chai": "2.8.0",
		"speculate": "^1.7.4",
		"standard": "^10.0.3",
		"style-loader": "^0.23.1",
		"url-loader": "^0.6.2",
		"webpack": "^3.12.0",
		"webpack-bundle-analyzer": "^3.4.1",
		"webpack-dev-server": "^2.11.5",
		"webpack-sources": "1.0.1"
	},
	"engines": {
		"node": ">=0.12.0"
	},
	"standard": {
		"globals": [
			"angular",
			"localStorage",
			"Blob",
			"sessionStorage",
			"FileReader",
			"$"
		]
	},
	"spec": {
		"requires": [
			"nodejs"
		],
		"environment": {
			"NODE_ENV": "production"
		},
		"post": [
			"npm install -g http-server"
		]
	},
	"scripts": {
		"start": "NODE_ENV=prod http-server ./dist -p 9000",
		"start:dev": "NODE_ENV=dev webpack-dev-server --hot=true",
		"clean": "rimraf dist",
		"lint": "standard 'app/**/*.js'",
		"lint:fix": "npm run lint -- --fix",
		"test": "karma start --single-run --browsers PhantomJS",
		"test:watch": "karma start --auto-watch",
		"prepare": "npm run clean && npm run build:prod",
		"build": "webpack",
		"build:prod": "NODE_ENV=prod NODE_OPTIONS=--max_old_space_size=4096 webpack",
		"version": "node versionManager.js && git add -u",
		"spec": "speculate"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/jembi/openhim-console.git"
	},
	"keywords": [
		"openhim",
		"openhie"
	],
	"author": "Jembi Health Systems NPC",
	"license": "MPL-2.0",
	"bugs": {
		"url": "https://github.com/jembi/openhim-console/issues"
	},
	"homepage": "http://openhim.org"
}
